uBA,EAAA,IAvBA,kBAuBA;AACA;AACA,WAAA;AAAA,MAAA,MAAA,EAAA,KAAA;AAAA,MAAA,IAAA,EAAA;AAAA,KAAA;AACA,GA1BA;AA2BA,EAAA,OA3BA,qBA2BA;AAAA;;AACA,IAAA,UAAA,CAAA,YAAA;AACA,MAAA,KAAA,CAAA,IAAA,GAAA,KAAA;AACA,KAFA,EAEA,IAFA,CAAA;AAGA,GA/BA;AAgCA,EAAA,OAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,WAAA,MAAA,GAAA,IAAA;AACA,KAHA;AAIA,IAAA,IAJA,kBAIA;AACA,WAAA,MAAA,GAAA,KAAA;AACA;AANA;AAhCA,CAAA","sourcesContent":["<template>\n  <div v-if=\"flag\">\n    <div class=\"box\" v-click-outside=\"hide\" ref=\"box\">\n      <input type=\"text\" @focus=\"show\" />\n      <div v-show=\"isShow\">面板</div>\n    </div>\n  </div>\n  <div v-else></div>\n</template>\n<script>\n// 指令特点可以复用  v-show  ， 核心就是控制dom，给dom元素绑定事件\n// 自定义指令核心就是操作dom   \n// 图片懒加载 、 虚拟滚动\n\n// vue简单 react 容易\nexport default {\n  name: \"clickOutSide\",\n  directives: {\n    clickOutside: {\n      // bind + update\n      bind(el, bindings, vnode) { // bindings 修饰符 和 值\n        const handler = (e) => {\n          if (!el.contains(e.target)) {\n            // 点击的是外边\n            let fn = vnode.context[bindings.expression]; // hide\n            fn();\n          }\n        };\n        el.handler = handler;\n        // 只要点击的不是box中的内容就隐藏掉\n        document.addEventListener(\"click\", handler);\n      },\n      unbind(el) {\n        console.log(\"remove\");\n        document.removeEventListener(\"click\", el.handler);\n      },\n    },\n  },\n  data() {\n    // 合并时\n    return { isShow: false,flag:true };\n  },\n  mounted() {\n      setTimeout(() => {\n            this.flag = false\n      }, 1000);\n  },\n  methods: {\n    show() {\n      this.isShow = true;\n    },\n    hide() {\n      this.isShow = false;\n    },\n  },\n};\n</script>\n\n<style>\n.box {\n  display: inline-flex;\n  flex-direction: column;\n  border: 1px solid red;\n}\n</style>\n\n"],"sourceRoot":"src/components/history"}]}