ate(valid => {\n        if (valid) {\n          alert(\"submit!\");\n        } else {\n          console.log(\"error submit!!\");\n          return false;\n        }\n      });\n    }\n  },\n  mounted(){\n    window.my = this.$data;\n  }\n};\n</script>"],"sourceRoot":"src/components"}]}