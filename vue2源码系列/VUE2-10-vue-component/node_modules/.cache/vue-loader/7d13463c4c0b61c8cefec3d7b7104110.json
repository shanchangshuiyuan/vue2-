加载 、 虚拟滚动\n\n// vue简单 react 容易\nexport default {\n  name: \"clickOutSide\",\n  directives: {\n    clickOutside: {\n      // bind + update\n      bind(el, bindings, vnode) { // bindings 修饰符 和 值\n        const handler = (e) => {\n          if (!el.contains(e.target)) {\n            // 点击的是外边\n            let fn = vnode.context[bindings.expression]; // hide\n            fn();\n          }\n        };\n        el.handler = handler;\n        // 只要点击的不是box中的内容就隐藏掉\n        document.addEventListener(\"click\", handler);\n      },\n      unbind(el) {\n        console.log(\"remove\");\n        document.removeEventListener(\"click\", el.handler);\n      },\n    },\n  },\n  data() {\n    // 合并时\n    return { isShow: false,flag:true };\n  },\n  mounted() {\n      setTimeout(() => {\n            this.flag = false\n      }, 1000);\n  },\n  methods: {\n    show() {\n      this.isShow = true;\n    },\n    hide() {\n      this.isShow = false;\n    },\n  },\n};\n</script>\n\n<style>\n.box {\n  display: inline-flex;\n  flex-direction: column;\n  border: 1px solid red;\n}\n</style>\n\n"]}]}